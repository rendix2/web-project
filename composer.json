{
  "name": "rendix2/web-project",
  "description": "Nette: Improved Web Project",
  "keywords": [
    "nette",
    "web-project",
    "php"
  ],
  "type": "project",
  "license": [
    "MIT",
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "homepage": "https://github.com/rendix2/web-project",
  "require": {
    "php": ">= 8.2",

    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-pdo_sqlite": "*",
    "ext-phar": "*",
    "ext-session": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",

    "contributte/console": "^0.10.1",
    "contributte/console-extra": "^0.8.0",
    "contributte/forms-bootstrap": "^0.8.3",
    "contributte/guzzlette": "^3.3",
    "contributte/menu-control": "^3.2",
    "contributte/translation": "^2.0",
    "contributte/webpack": "^2.3",
    "ublaboo/datagrid": "dev-next",

    "freezy-bee/doctrine-form-mapper": "^1.7",
    "latte/latte": "^3.0",
    "tracy/tracy": "^2.10",

    "nette/application": "^3.2.3",
    "nette/bootstrap": "^3.2",
    "nette/caching": "^3.3",
    "nette/di": "^3.2",
    "nette/forms": "^3.2",
    "nette/http": "^3.3",
    "nette/mail": "^4.0",
    "nette/robot-loader": "^4.0",
    "nette/security": "^3.2",
    "nette/utils": "^4.0",
    
    "nettrine/cache": "^0.3.0",
    "nettrine/dbal": "^0.8.2",
    "nettrine/extensions-beberlei": "^0.2.0",
    "nettrine/fixtures": "^0.7.2",
    "nettrine/migrations": "^0.9.1",
    "nettrine/orm": "^0.8.4"
  },
  "require-dev": {
    "dg/composer-frontline": "^0.2.0",
    "nette/tester": "^2.5",
    "ninjify/coding-standard": "^0.12.1",
    "roave/security-advisories": "dev-latest",
    "symfony/thanks": "^1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "symfony/thanks": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "dg/composer-frontline": true
    },
    "optimize-autoloader": true,
    "sort-packages": true,
    "lock": true
  }
}
